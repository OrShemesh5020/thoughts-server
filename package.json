{
  "name": "node_project",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run migrate && npm run server-start && npm run logs",
    "stop": "pm2 delete all",
    "logs": "pm2 logs",
    "server-start": "nodemon src/server.js --exec babel-node",
    "restart": "npm stop && npm start",
    "build": "babel src/ -d dist/",
    "serve": "pm2 start dist/server.js",
    "migrate": "sequelize-cli db:migrate --config src/config/database-config.js",
    "migrate-undo": "sequelize-cli db:migrate:undo --config src/config/database-config.js",
    "migrate-create": "sequelize-cli migration:create"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "pg": "^8.3.3",
    "pg-hstore": "^2.3.3",
    "pm2": "^4.5.0",
    "postgres": "^1.0.2",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.12.1",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "nodemon": "^2.0.6"
  }
}
